{
  "manifest_version": 2,

  "name": "#loremipsum",
  "description": "Generate kool hashtags for your Instagram uploads",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://instagram.com/",
    "tabs",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": ["*://instagram.com/p/*"],
      "css": ["css/main.css"],
      "js": ["js/external/jquery.min.js", "js/dom.js"],
      "run_at": "document_end"
    }
  ]
}
